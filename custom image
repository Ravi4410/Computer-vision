import numpy as np
import cv2
custom = np.zeros((300,300,3),dtype = "uint8")

custom = cv2.rectangle(custom,(40,40),(200,200),(220,0,0),10)
custom = cv2.rectangle(custom,(150,150),(250,250),(0,245,0),10)
custom = cv2.rectangle(custom,(100,100),(190,190),(0,0,245),10)

#custom[20:280,20:220] = [[243]]
cv2.imshow("image",custom)
cv2.waitKey()
cv2.destroyAllWindows()
